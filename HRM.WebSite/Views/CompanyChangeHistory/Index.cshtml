@model IEnumerable<HRM.ViewModels.Company.CompanyChangeHistoryViewModel>
<div class="row">
    <div class="col-md-12">
        <!-- BEGIN EXAMPLE TABLE PORTLET-->
        <div class="portlet light bordered">
            <div class="portlet-body">
                <div class="table-toolbar">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="btn-group">
                                <button id="sample_editable_1_new" class="btn sbold green">
                                    @Html.ActionLink("Thêm mới", "Create")
                                    <i class="fa fa-plus"></i>
                                </button>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="btn-group pull-right">
                                <button class="btn green  btn-outline dropdown-toggle" data-toggle="dropdown">
                                    Tools
                                    <i class="fa fa-angle-down"></i>
                                </button>
                                <ul class="dropdown-menu pull-right">
                                    <li>
                                        <a href="javascript:;">
                                            <i class="fa fa-print"></i> Print
                                        </a>
                                    </li>
                                    <li>
                                        <a href="javascript:;">
                                            <i class="fa fa-file-pdf-o"></i> Save as PDF
                                        </a>
                                    </li>
                                    <li>
                                        <a href="javascript:;">
                                            <i class="fa fa-file-excel-o"></i> Export to Excel
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <table class="table table-striped table-bordered table-hover table-checkable order-column" id="sample_1">
                    <thead>
                        <tr class="mautable">
                            <th>
                                <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                    <input type="checkbox" class="group-checkable" data-set="#sample_1 .checkboxes" />
                                    <span></span>
                                </label>
                            </th>
                            <th>Tên tiếng việt</th>
                            <th>Tên tiếng anh</th>
                            <th>Tên viết tắc</th>
                            <th>Mã số thuế</th>
                            <th>Địa chỉ</th>
                            <th>Ngày đăng ký</th>
                            <th>Vốn điều lệ</th>
                            <th>Số lần đăng ký</th>
                            <th>Nội dung thay đổi</th>
                            <th>Website</th>
                            <th>Điện thoại bàn</th>
                            <th>Fax</th>
                            <th>Di động</th>
                            <th>Email</th>
                            <th>Số tài khoản</th>
                            <th>Ngân hàng</th>
                            <th>Người đại diện pháp luật</th>
                            <th>Ảnh GPKD</th>
                            <th>Chức năng</th>

                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                        <tr class="odd gradeX">
                            <td>
                                <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                    <input type="checkbox" class="checkboxes" value="1" />
                                    <span></span>
                                </label>
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.VietNamName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.EnglishName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ShortName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.TaxCode)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Address)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ChangedDate)
                            </td>
                            <td class="money">
                                @Html.DisplayFor(modelItem => item.CharterCapital)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ChangedCount)
                            </td>

                            <td>
                                @Html.DisplayFor(modelItem => item.ChangedContent)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Website)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Mobile)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Fax)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Phone)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Email)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.BankAccountNumber)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.BankAccountIssuePlace)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.SignName)
                            </td>
                            <td>
                                <a href="@Html.DisplayFor(modelItem => item.SignTitle)"> @Html.DisplayFor(modelItem => item.SignTitle)</a>
                            </td>
                            <td>
                                @Html.ActionLink("Sửa", "Edit", new { id = item.Id }) |
                                @Html.ActionLink("Xóa", "Delete", new { id = item.Id })
                            </td>

                        </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <!-- END EXAMPLE TABLE PORTLET-->
    </div>
</div>
