@section Scripts
{
    @if (Model.ChangedDate.HasValue)
    {
        <script type="text/javascript">
    $(document).ready(function () {
        setDate("ChangedDate", @Model.ChangedDate.Value.ToJavaScriptMilliseconds());
            });
        </script>
    }

}